#!/usr/bin/env node
'use strict'

const meow = require('meow')

const { main } = require('../')

const template = `
	Usage
	  $ dadjokes

	Options
	  --help, -h     Show help
	  --version, -v  Show version
`

const options = {
  flags: {
    help: {
      type: 'boolean',
      alias: 'h',
    },
    version: {
      type: 'boolean',
      alias: 'v',
    },
  },
}

meow(template, options)

main()
